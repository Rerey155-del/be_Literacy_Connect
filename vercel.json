{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "rewrites": [
      { "source": "/donatur", "destination": "/donatur" }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "devCommand": "npm run dev",
    "buildCommand": "npm run build"
  }